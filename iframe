    private void jButton29ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        String cadena;
        cadena=txtpantalla.getText();
        
        if (cadena.length()>0){
        cadena=cadena.substring(0,cadena.length()-1);
        txtpantalla.setText(cadena);
        }
    }                                         

